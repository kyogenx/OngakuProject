@model User
@{
    ViewData["Title"] = "Profile";
    User? UserInfo = ViewBag.UserInfo;   
}

<div class="box-sm-part shadow-sm">
    <div class="d-none">
        <input type="hidden" id="CurrentEmail_Val" value="@UserInfo.Email" />
        <form method="post" asp-controller="Profile" asp-action="EditImages" id="EditProfileImages_Form" enctype="multipart/form-data">
            <input asp-for="ImgUrl" type="file" name="Files" id="EditImage_Files_Val" data-trigger="EditProfileImages_Form" data-order-target="EditImage_Files_Val" accept="image/png, image/gif, image/jpeg" multiple />
        </form>
        <form method="get" asp-controller="Profile" asp-action="GetImagesQty" id="GetImagesQty_Form">
            <input type="hidden" name="Id" value="@UserInfo.Id" />
        </form>
        <form method="get" asp-controller="Profile" asp-action="GetImage" id="ProfileGetImage_Form">
            <input type="hidden" name="Skip" id="PGI_Skip_Val" value="0" />
        </form>
        <form method="get" asp-controller="Country" asp-action="GetCountries" id="GetCountries_Form">
            <input type="hidden" name="Keyword" id="GetCountries_Keyword_Val" />
        </form>
    </div>
    <div class="profile-info-box text-center">
        <div class="standard-profile-bar">
            <div class="float-start">
                <div class="dropdown">
                    <button class="btn btn-standard" type="button" data-bs-toggle="dropdown" aria-expanded="false"> <i class="fa-solid fa-ellipsis"></i> </button>
                    <ul class="dropdown-menu shadow-sm p-1">
                        <li><button type="button" class="dropdown-item btn-edit-profile-image" onmousedown="$('#EditImage_Files_Val').mousedown();"> <i class="fa-solid fa-image"></i> Edit Image</button></li>
                        <li>
                            <form method="post" asp-controller="Account" asp-action="LogOut" id="LogOut_Form">
                                <button type="submit" class="dropdown-item text-danger"> <i class="fa-solid fa-right-from-bracket"></i> Log Out</button>
                            </form>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="float-end ms-1">
                <button type="button" class="btn btn-standard btn-open-container btn-tooltip" id="EditProfile_Container-Open_Btn" data-bs-toggle="tooltip" data-bs-placement="bottom" data-bs-custom-class="tooltip-standard shadow-sm" data-bs-title="Edit nickname, searchname and/or account's description"> <i class="fa-solid fa-pencil"></i> </button>
            </div>
        </div>
        <div class="standard-image-bar" style="display: none;">
            <div class="hstack gap-1">
                <div class="dropdown">
                    <button class="btn btn-standard" type="button" data-bs-toggle="dropdown" aria-expanded="false"> <i class="fa-solid fa-ellipsis"></i> </button>
                    <ul class="dropdown-menu shadow-sm p-1">
                        <li>
                            <form method="post" asp-controller="Profile" asp-action="SetImageAsMain" id="ProfileSetImageAsMain_Form">
                                <input type="hidden" name="ImageUrl" id="PSIAM_ImgUrl_Val" />
                                <button type="submit" class="dropdown-item btn-edit-some-files super-disabled" id="PSIAM_SbmtBtn"> <i class="fa-solid fa-user-check"></i> Set as Main</button>
                            </form>
                        </li>
                        <li>
                            <form method="post" asp-controller="Profile" asp-action="DeleteImage" id="ProfileDeleteImage_Form">
                                <input type="hidden" name="ImageUrl" id="PDI_ImgUrl_Val" />
                                <button type="submit" class="dropdown-item btn-edit-some-files text-danger super-disabled" id="ProfileDeleteImage_SbmtBtn"> <i class="fa-solid fa-trash-can"></i> Delete this Image</button>
                            </form>
                        </li>
                        <li>
                            <form method="post" asp-controller="Profile" asp-action="DeleteAllImages" id="ProfileDeleteAllImages_Form">
                                <button type="submit" class="dropdown-item btn-edit-some-files text-danger super-disabled" id="ProfileDeleteAllImages_SbmtBtn"> <i class="fa-solid fa-dumpster"></i> Delete all Images</button>
                            </form>
                        </li>
                    </ul>
                </div>
                <button type="button" class="btn btn-standard btn-exit-photo-mode ms-auto"> <i class="fa-solid fa-compress"></i> </button>
            </div>
            <div class="row" id="AvatarsCounter_Box">
            </div>
        </div>
        <div class="mt-2">
            @if (UserInfo.ImgUrl == null)
            {
                <div class="d-none" id="ImgHdnValues_Box">
                    <input type="hidden" id="ImagesQty_Val" value="0" />
                </div>
                <div class="profile-avatar mx-auto">@UserInfo.Nickname![0]</div>
                <img src="#" class="profile-avatar-img mx-auto" alt="This image cannot be displayed" id="ProfileAvatar_Img" style="display: none;" />
            }
            else
            {
                <div class="d-none" id="ImgHdnValues_Box">
                    <input type="hidden" id="ImagesQty_Val" value="0" />
                    <input type="hidden" class="form-control-img" id="0-ImgHdn_Val" value="@UserInfo.ImgUrl" />
                </div>
                <div class="profile-avatar mx-auto" style="display: none;">@UserInfo.Nickname![0]</div>
                <img src="/ProfileImages/@UserInfo.ImgUrl" class="profile-avatar-img mx-auto" alt="This image cannot be displayed" id="ProfileAvatar_Img" />
            }
        </div>
        <div class="standard-profile-initials-box mt-2">
            <h2 class="h2" id="ProfileMainName_Lbl">@UserInfo.Nickname</h2>
            <p class="card-text text-muted">@@<span id="ProfileMainSearchname_Lbl">@UserInfo.Searchname</span></p>
        </div>
    </div>
    <div class="box-btn-group mt-2">
        <div>
            <form method="get" action="/Profile/GetAccountInfo" id="GetAccountInfo_Form">
                <button type="submit" class="btn box-btn-group-member box-btn-group-top-member hstack gap-1" id="GetProfileInfo_SbmtBtn">
                    <div class="box-btn-group-member-icon"> <i class="fa-regular fa-address-card"></i> </div>
                    <span class="card-text ms-1">Personal Info</span>
                    <small class="card-text text-muted ms-auto"> <i class="fa-solid fa-angle-right"></i> </small>
                </button>
            </form>
        </div>
        <div>
            <form method="get" action="/Profile/GetAccountGuts" id="GetAccountGuts_Form">
                <button type="submit" class="btn box-btn-group-member box-btn-group-mid-member hstack gap-1" id="GetAccountGuts_SbmtBtn">
                    <div class="box-btn-group-member-icon"> <i class="fa-solid fa-fingerprint"></i> </div>
                    <span class="card-text ms-1">Security Settings</span>
                    <small class="card-text text-muted ms-auto"> <i class="fa-solid fa-angle-right"></i> </small>
                </button>
            </form>
        </div>
        <div>
            <form method="get" asp-controller="Profile" asp-action="GetPrivacySettings" id="GetPrivacySettings_Form">
                <input type="hidden" name="Id" value="@UserInfo.Id" />
                <button type="submit" class="btn box-btn-group-member box-btn-group-mid-member hstack gap-1" id="GetPrivacySettings_SbmtBtn">
                    <div class="box-btn-group-member-icon"> <i class="fa-solid fa-shield-halved"></i> </div>
                    <span class="card-text ms-1">Privacy Settings</span>
                    <small class="card-text text-muted ms-auto"> <i class="fa-solid fa-angle-right"></i> </small>
                </button>
            </form>
        </div>
        <div>
            <form method="get" action="/Poll/GetByUser" id="GetUserPolls_Form">
                <input type="hidden" name="Type" id="GetUserPolls_Type_Val" value="0" />
                <button type="submit" class="btn box-btn-group-member box-btn-group-bot-member hstack gap-1" id="GetUserPolls_SbmtBtn">
                    <div class="box-btn-group-member-icon"> <i class="fa-solid fa-square-poll-vertical"></i> </div>
                    <span class="card-text ms-1">Your Polls</span>
                    <small class="card-text text-muted ms-auto"> <i class="fa-solid fa-angle-right"></i> </small>
                </button> 
            </form>
        </div>
@*         <button type="button" class="btn box-btn-group-member box-btn-group-mid-member btn-open-container text-start" id="ProfileSecurity_Container-Open"> <i class="fa-solid fa-fingerprint"></i> Security Settings <small class="card-text text-muted float-end"> <i class="fa-solid fa-angle-right"></i> </small></button> *@
    </div>
    <div class="box-btn-group mt-2">
        <button type="button" class="btn box-btn-group-member box-btn-group-top-member hstack gap-1">
            <div class="box-btn-group-member-icon"> <i class="fa-solid fa-id-card"></i> </div>
            <span class="card-text ms-1">Preview</span>
            <small class="card-text text-muted ms-auto"> <i class="fa-solid fa-angle-right"></i> </small>
        </button>
        <button type="button" class="btn box-btn-group-member box-btn-group-bot-member hstack gap-1">
            <div class="box-btn-group-member-icon"> <i class="fa-solid fa-bell"></i> </div>
            <span class="card-text ms-1">Notifications</span>
            <small class="card-text text-muted ms-auto"> <i class="fa-solid fa-angle-right"></i> </small>
        </button>
    </div>
</div>

<div class="box-lg-part shadow-sm" id="Primary_Container">
    <div class="box-lg-part-header p-2">
        <div class="hstack gap-1">
            <div class="dropdown">
                <button class="btn btn-standard me-1" type="button" data-bs-toggle="dropdown" aria-expanded="false"> <i class="fa-solid fa-ellipsis"></i> </button>
                <ul class="dropdown-menu shadow-sm p-1">
                    <li><button type="button" class="dropdown-item btn-sm">New post <span class="card-text float-end"> <i class="fa-solid fa-feather"></i> </span></button></li>
                    <li><button type="button" class="dropdown-item btn-sm">New poll <span class="card-text float-end"> <i class="fa-solid fa-square-poll-vertical"></i> </span></button></li>
                    <li><button type="button" class="dropdown-item btn-open-container btn-sm" id="AddMusic_Container-Open">Add Music <span class="card-text float-end"> <i class="fa-solid fa-microphone-lines"></i> </span></button></li>
                    <li>
                        <div>
                            <form method="get" asp-controller="Track" asp-action="GetStudioItems" id="GetStudioItems_Form">
                                <button type="submit" class="dropdown-item btn-sm">Studio <span class="card-text float-end"> <i class="fa-solid fa-music"></i> </span></button>
                            </form>
                        </div>
                    </li>
                    <li><hr class="dropdown-divider" /></li>
                    <li><button type="button" class="dropdown-item btn-sm" id="ShowBasicShortcutsInfo_Btn">Shortcuts <span class="card-text float-end"> <i class="fa-regular fa-lightbulb"></i> </span></button></li>
                </ul>
            </div>
            <div class="d-inline-block">
                <form method="get" asp-controller="Playlists" asp-action="GetFavorites" id="GetFavorites_Form">
                    <button type="submit" class="btn btn-standard" id="GetFavorites_SbmtBtn"> <i class="fa-solid fa-star"></i>  Favorites</button>
                </form>
            </div>
        </div>
    </div>
    <div class="box-lg-part-body mt-5" id="Primary_Box">
        <div class="mt-2">
            <div class="box-bordered p-2 pt-1">
                <button type="button" class="btn btn-standard btn-link btn-sm super-disabled float-end ms-1" id="EditResults_WebsiteLink_Btn"> <i class="fa-solid fa-link"></i> <span id="EditResults_WebsiteLink_Span">Link</span></button>
                <span class="h1" id="EditResults_Nickname_Lbl">K. Flay</span>
                <br/>
                <small class="card-text text-muted" id="EditResults_Misch_Span"><span class="separator-dot" id="EditResults_Location_Span">from United States of America</span> <span id="EditResults_Location_Span-Separator" style="display: none;">∙</span> <span class="separator-dot" id="EditResults_FormedAt_Span" style="display: none;">30 June 1985</span> <span id="EditResults_FormedAt_Span-Separator" style="display: none;">∙</span> <span class="separator-dot" id="EditResults_MainGenre_Span" style="display: none;">Alternative</span></small>
                <div class="box-border-top mt-2 pt-1" id="EditResults_Description_Box">
                    <small class="card-text white-space-on" id="EditResults_Description_Span">Description of Artist Profile</small>
                </div>
            </div>
            <div class="box-standard mt-3" id="DescriptionEdit_Box">
                <div class="hstack gap-2">
                    <h6 class="h6 ms-1">Bio</h6>
                    <button type="button" class="btn btn-distance-submitter btn-sm ms-auto" data-form="EditUserBio_Form" id="EditUser_Bio_Val-DistantSbmt_Btn"><i class="fa-regular fa-circle-check"></i> Saved</button>
                </div>
                <button type="button" class="btn btn-select-primary-skin btn-solo-input call-text-editor slide-element active" id="EditUserDescription_Call_Btn" data-input="BioEdit_Box" data-text-box="EditUser_Bio">Artist Bio ∙ <span class="text-muted" id="EditUserWebsite_Webpage_Val-Indicator_Span">0/1500</span> <span class="float-end text-muted ms-1"> <i class="fa-regular fa-keyboard"></i> </span></button>
                <div class="slide-element" id="BioEdit_Box" style="display: none;">
                    <div class="hstack gap-2">
                        <div class="w-100">
                            <div class="form-textarea form-control-undetermined form-control-select-like form-control-guard" contenteditable="true" maxlegth="1500" data-min-length="1" placeholder="Your bio (max 1500 chars)" id="EditUser_Bio"></div>
                            <form method="post" action="/Profile/EditBio" id="EditUserBio_Form">
                                <input type="hidden" class="form-control-distance" id="EditUser_Bio_Val" name="Description" placeholder="Your bio (max 1500 chars)" maxlength="1500" />
                            </form>
                            <input type="hidden" id="EditUser_Bio_Val-Base_Val" />
                        </div>
                        <button type="button" class="btn btn-standard-rounded btn-change-elements hide-text-bar ms-auto" id="UserWebsiteLink_Uncall_Btn" data-close="BioEdit_Box" data-open="EditUserDescription_Call_Btn"> <i class="fa-solid fa-xmark"></i> </button>
                    </div>
                </div>
                <div class="mt-1 ms-1">
                    <small class="card-text text-muted">Introduce yourself with a short bio — share your story, influences, achievements, and what defines your music</small>
                </div>
            </div>
            <div class="box-standard mt-3" id="ArtistType_Box">
                <div class="hstack gap-2">
                    <h6 class="h6 ms-1">Artist Type</h6>
                    <button type="button" class="btn btn-distance-submitter btn-sm ms-auto" data-form="EditUserType_Form" id="EditUserType_Type_Val-DistantSbmt_Btn"> <i class="fa-regular fa-circle-check"></i> Saved</button>
                </div>
                <button type="button" class="btn btn-select-primary-skin btn-select-primary" id="ArtistType-Select_Btn" data-val="0"><span id="ArtistType_Result_Span">Solo Artist</span> <span class="float-end text-muted ms-1"> <i class="fa-solid fa-angle-down"></i> </span></button>
                <div class="d-none">
                    <form method="post" action="/Profile/EditType" id="EditUserType_Form">
                        <input type="hidden" class="form-control-distance" name="Type" id="EditUserType_Type_Val" value="0" />
                    </form>
                    <input type="hidden" id="EditUserType_Type_Val-Base_Val" value="0" />
                </div>
                <div class="mt-1 ms-1">
                    <small class="card-text text-muted ">Specify the type of artist you are. This determines how your profile is displayed and which features are available to you</small>
                </div>
            </div>

            <div class="box-standard mt-3" id="ArtistMainGenre_Box">
                <div class="hstack gap-2">
                    <h6 class="h6 ms-1">Main Genre</h6>
                    <button type="button" class="btn btn-distance-submitter btn-sm ms-auto" data-form="EditUserMainGenre_Form" id="EditUserMainGenre_Id_Val-DistantSbmt_Btn"> <i class="fa-regular fa-circle-check"></i> Saved</button>
                </div>
                <div>
                    <form method="get" action="/Genre/Search" id="SearchForGenres_Form">
                        <input type="hidden" name="Type" id="SearchForGenres_Type_Val" value="1" />
                        <input type="hidden" name="Keyword" id="SearchForGenres_Keyword_Val" />
                        <button type="submit" class="btn btn-select-primary-skin btn-select-primary" id="SearchForGenre_SbmtBtn"><span id="UserMainGenre_Span">Unknown</span> <span class="float-end text-muted ms-1"> <i class="fa-solid fa-angle-down"></i> </span></button>
                    </form>
                </div>
                <div class="d-none">
                    <form method="post" action="/Profile/EditMainGenre" id="EditUserMainGenre_Form">
                        <input type="hidden" class="form-control-distance" name="Id" id="EditUserMainGenre_Id_Val" value="0" />
                        <input type="hidden" name="Type" value="1" />
                    </form>
                    <input type="hidden" id="EditUserMainGenre_Id_Val-Base_Val" value="0" />
                </div>
                <div class="mt-1 ms-1">
                    <small class="card-text text-muted ">These tags describe your musical style and help fans discover your work</small>
                </div>
            </div>

            <div class="box-standard mt-3" id="YearsOfActivity_Box">
                <div class="hstack gap-2">
                    <h6 class="h6 ms-1" id="YearsOfActivity_Header_Lbl">Born/Formed</h6>
                    <button type="button" class="btn btn-distance-submitter btn-sm ms-auto" data-form="EditUserYearsOfActivity_Form" id="EditYearsOfActivity_DateTime_Val-DistantSbmt_Btn"> <i class="fa-regular fa-circle-check"></i> Saved</button>
                </div>
                <button type="button" class="btn btn-select-primary-skin btn-select-date" id="YearsOfActivity_Call_Btn" data-display="YearsOfActivity_Result_Span" data-result="EditYearsOfActivity_DateTime_Val"><span id="YearsOfActivity_Result_Span">Unknown</span> <span class="float-end text-muted ms-1"> <i class="fa-regular fa-clock"></i> </span></button>
                <div class="mt-1 ms-1">
                    <small class="card-text text-muted">Let listeners know when you started releasing music</small>
                </div>
                <div class="d-none">
                    <form method="post" action="/Profile/EditYearsOfActivity" id="EditUserYearsOfActivity_Form">
                        <input type="hidden" class="form-control-distance" name="FormedAt" id="EditYearsOfActivity_DateTime_Val" />
                    </form>
                    <input type="hidden" id="EditYearsOfActivity_DateTime_Val-Base_Val" />
                </div>
            </div>

            <div class="box-standard mt-3" id="ArtistLocation_Box">
                <div class="hstack gap-2">
                    <h6 class="h6 ms-1" id="ArtistLocation_Header_Lbl">Location</h6>
                    <button type="button" class="btn btn-distance-submitter btn-sm ms-auto" data-form="EditUserLocaion_Form" id="EditUserLocaion_Id_Val-DistantSbmt_Btn"> <i class="fa-regular fa-circle-check"></i> Saved</button>
                </div>
                <div>
                    <form method="get" action="/Country/Get" id="GetCountries_Form">
                        <input type="hidden" name="Type" value="1" />
                        <input type="hidden" name="Id" id="GetCountries_Id_Val" value="0" />
                        <button type="submit" class="btn btn btn-select-primary-skin" id="GetCountries_SbmtBtn"><span id="UserCountryInfo_Btn_Span">Unknown</span> <span class="float-end text-muted ms-1"> <i class="fa-solid fa-angle-down"></i> </span></button>
                    </form>
                </div>
                <div class="mt-1 ms-1">
                    <small class="card-text text-muted">Choose the country you’re based in or where your project was formed</small>
                </div>
                <div class="d-none">
                    <form method="post" action="/Profile/EditLocation" id="EditUserLocaion_Form">
                        <input type="hidden" class="form-control-distance" name="Id" id="EditUserLocaion_Id_Val" value="0" />
                    </form>
                    <input type="hidden" id="EditUserLocaion_Id_Val-Base_Val" value="0" />
                </div>
            </div>

            <div class="box-standard mt-3" id="UserWebsiteLink_Box">
                <div class="hstack gap-2">
                    <h6 class="h6 ms-1" id="UserWebsiteLink_Header_Lbl">Official Website</h6>
                    <button type="button" class="btn btn-distance-submitter btn-sm ms-auto" data-form="EditUserWebsite_Form" id="EditUserWebsite_Webpage_Val-DistantSbmt_Btn"> <i class="fa-regular fa-circle-check"></i> Saved</button>
                </div>
                <button type="button" class="btn btn-select-primary-skin btn-solo-input slide-element active" id="UserWebsiteLink_Call_Btn" data-input="WebsiteEdit_Box"><span id="UserWebsiteLink_Info_Span">No Website Link</span> <span class="float-end text-muted ms-1"> <i class="fa-solid fa-link"></i> </span></button>
                <div class="slide-element" id="WebsiteEdit_Box" style="display: none;">
                    <div class="hstack gap-2">
                        <div class="w-100">
                            <form method="post" action="/Profile/EditWebsite" id="EditUserWebsite_Form">
                                <input type="text" class="form-control-select-like form-control-guard form-control-distance" id="EditUserWebsite_Webpage_Val" name="Webpage" placeholder="Website link..." data-update="UserWebsiteLink_Info_Span" data-base-value="No Website Link" />
                            </form>
                            <input type="hidden" id="EditUserWebsite_Webpage_Val-Base_Val" />
                        </div>
                        <button type="button" class="btn btn-standard-rounded btn-change-elements ms-auto" id="UserWebsiteLink_Uncall_Btn" data-close="WebsiteEdit_Box" data-open="UserWebsiteLink_Call_Btn"> <i class="fa-solid fa-xmark"></i> </button>
                    </div>
                </div>
                <div class="mt-1 ms-1">
                    <small class="card-text text-muted">Share your official website (if any) to give fans more ways to connect with you</small>
                </div>
            </div>
        </div>
    </div>
</div>

